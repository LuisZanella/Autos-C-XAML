<Window x:Class="Agencia.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Agencia"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Canvas HorizontalAlignment="Left" Height="319" VerticalAlignment="Top" Width="517">
            <RadioButton x:Name="rdbANuevo" GroupName="TipoAuto" Content="Autos Nuevos" Canvas.Left="187" Canvas.Top="77" Checked="rdbANuevo_Checked"/>
            <RadioButton x:Name="rdbAUsado" GroupName="TipoAuto" Content="Autos Usados" Canvas.Left="187" Canvas.Top="97" Checked="rdbAUsado_Checked"/>
        </Canvas>
        <Label Content="Agencia de Autos Bara Bara" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="200,10,0,0" FontWeight="Bold"/>

    </Grid>
</Window>
